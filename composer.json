{
    "name": "laravel-notification-channels/pubnub",
    "description": "PubNub Notifications Channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/pubnub",
    "license": "MIT",
    "authors": [
        {
            "name": "iWader",
            "email": "wade@iwader.co.uk",
            "homepage": "https://iwader.co.uk/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/queue": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "pubnub/pubnub": "^3.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0 || ~8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
