{
    "name": "laravel-notification-channels/pubnub",
    "description": "PubNub Notifications Channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/pubnub",
    "license": "MIT",
    "authors": [
        {
            "name": "iWader",
            "email": "wade@iwader.co.uk",
            "homepage": "https://iwader.co.uk/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/notifications": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.0",
        "pubnub/pubnub": "^4.1.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pubnub\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
